    "autoload": {
        "psr-4": {
            "Differ\\": "src/"
        },
        "files": [
            "src/Differ.php",
            "src/GenerateAst.php",
            "src/Parser.php",
            "src/Formatters/Stylish.php",
            "src/Formatters/Plain.php"
            ]

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Gendiff\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "nbadin",
